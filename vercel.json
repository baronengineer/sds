{
    "builds": [
        {
            "src": "blog/wsgi.py",
            "use": "@vercel/python",
            "config": {"maxLambdaSize": "15mb", "runtime": "python3.9"}
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/staticbuild",
            "config":{
                "distDir":"staticfiles_bulid"
            }
        }
    ],
    "routes": [
        {
            "src": "/staticfiles/(.*)",
            "dest": "staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "blog/wsgi.py"
        }
    ]
}
