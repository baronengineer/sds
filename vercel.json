{
    "builds": [
        {
            "src": "blog/wsgi.py",
            "use": "@vercel/python",
            "config": { "runtime": "python3.9"}
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "blog/wsgi.py"
        }
    ]
}

